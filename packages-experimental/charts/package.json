{
    "name": "charts",
    "widgetName": "Charts",
    "version": "0.0.1",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/mendix/native-mobile-resources.git"
    },
    "config": {
        "widgetPath": "../../packages-native/test-project/mxproject/widgets",
        "projectPath": "../../packages-native/test-project/mxproject",
        "subprojectFolder": "packages-experimental/charts"
    },
    "packagePath": "com.mendix.widget.native",
    "scripts": {
        "start": "pluggable-widgets-tools start:ts:native --subprojectPath $npm_package_config_subprojectFolder",
