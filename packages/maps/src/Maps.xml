<?xml version="1.0" encoding="utf-8"?>
<widget id="com.mendix.widget.native.maps.Maps" supportedPlatform="All" needsEntityContext="true" offlineCapable="true" pluginWidget="true" xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../../../xsd/widget.xsd">
    <name>Maps</name>
    <description>This widget allows you to use native Google or Apple maps views in your application.</description>
    <icon></icon>
    <properties>
        <propertyGroup caption="Markers">
            <propertyGroup caption="Dynamic marker">
                <property key="markerTitle" type="attribute" required="false">
                    <caption>Title</caption>
                    <description/>
                    <attributeTypes>
                        <attributeType name="String"/>
                    </attributeTypes>
                </property>
                <property key="markerDescription" type="attribute" required="false">
                    <caption>Description</caption>
                    <description/>
                    <attributeTypes>
                        <attributeType name="String"/>
                    </attributeTypes>
                </property>
                <property key="markerAddress" type="attribute" required="false">
                    <caption>Address</caption>
                    <description>The widget will try to geocode the address to coordinates, and save those to the latitude and longitude attributes, if provided. The address is ignored if latitude and longitude are set.</description>
                    <attributeTypes>
                        <attributeType name="String"/>
                    </attributeTypes>
                </property>
                <property key="markerLatitude" type="attribute" required="false">
                    <caption>Latitude</caption>
                    <description/>
                    <attributeTypes>
                        <attributeType name="Decimal"/>
                    </attributeTypes>
                </property>
                <property key="markerLongitude" type="attribute" required="false">
                    <caption>Longitude</caption>
                    <description/>
                    <attributeTypes>
                        <attributeType name="Decimal"/>
                    </attributeTypes>
                </property>
            </propertyGroup>
            <propertyGroup caption="Static markers">
                <property key="markers" type="object" isList="true" required="false">
                    <caption>Marker list</caption>
                    <description/>
                    <properties>
                        <property key="title" type="string" required="false">
                            <caption>Title</caption>
                            <category>Marker</category>
                            <description/>
                        </property>
                        <property key="description" type="string" required="false">
                            <caption>Description</caption>
                            <category>Marker</category>
                            <description/>
                        </property>
                        <property key="latitude" type="string">
                            <caption>Latitude</caption>
                            <category>Marker</category>
                            <description/>
                        </property>
                        <property key="longitude" type="string">
                            <caption>Longitude</caption>
                            <category>Marker</category>
                            <description/>
                        </property>
                        <property key="action" type="action" required="false">
                            <caption>On marker press</caption>
                            <category>Marker</category>
                            <description/>
                        </property>
                    </properties>
                </property>
            </propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Region">
            <property key="latitude" type="attribute">
                <caption>Latitude</caption>
                <description/>
                <attributeTypes>
                    <attributeType name="Decimal"/>
                </attributeTypes>
            </property>
            <property key="longitude" type="attribute">
                <caption>Longitude</caption>
                <description>The center coordinates of the region to be displayed on the map.</description>
                <attributeTypes>
                    <attributeType name="Decimal"/>
                </attributeTypes>
            </property>
            <property key="latitudeDelta" type="attribute">
                <caption>Latitude delta</caption>
                <description/>
                <attributeTypes>
                    <attributeType name="Decimal"/>
                </attributeTypes>
            </property>
            <property key="longitudeDelta" type="attribute">
                <caption>Longitude delta</caption>
                <description>The span of coordinates that is displayed around the center coordinates.</description>
                <attributeTypes>
                    <attributeType name="Decimal"/>
                </attributeTypes>
            </property>
        </propertyGroup>
        <propertyGroup caption="Behavior">
            <propertyGroup caption="Display">
                <property key="mapType" type="enumeration" defaultValue="standard">
                    <caption>Map type</caption>
                    <description/>
                    <enumerationValues>
                        <enumerationValue key="standard">Standard road map</enumerationValue>
                        <enumerationValue key="satellite">Satellite view</enumerationValue>
                        <enumerationValue key="hybrid">Satellite view with roads and points of interest overlayed</enumerationValue>
                    </enumerationValues>
                </property>
                <property key="provider" type="enumeration" defaultValue="default">
                    <caption>Provider</caption>
                    <description>Map provider. The default uses Apple Maps on iOS and Google Maps on Android.</description>
                    <enumerationValues>
                        <enumerationValue key="default">Default</enumerationValue>
                        <enumerationValue key="google">Google Maps</enumerationValue>
                    </enumerationValues>
                </property>
                <property key="showsUserLocation" type="boolean" defaultValue="false">
                    <caption>Show user location</caption>
                    <description>If enabled the app will ask for permission to access the user's location.</description>
                </property>
                <property key="showsMyLocationButton" type="boolean" defaultValue="true">
                    <caption>Show 'my location' button</caption>
                    <description>Show the button to move map to the current user's location.</description>
                </property>
            </propertyGroup>
            <propertyGroup caption="Zoom">
                <property key="zoomEnabled" type="boolean" defaultValue="true">
                    <caption>Enable zoom</caption>
                    <description>Allow the user to pinch/zoom the map.</description>
                </property>
                <property key="minZoomLevel" type="integer" defaultValue="1">
                    <caption>Minimum zoom value</caption>
                    <description>1: World, 5: Landmass/continent, 10: City, 15: Streets, 20: Buildings</description>
                </property>
                <property key="maxZoomLevel" type="integer" defaultValue="20">
                    <caption>Maximum zoom value</caption>
                    <description>1: World, 5: Landmass/continent, 10: City, 15: Streets, 20: Buildings</description>
                </property>
            </propertyGroup>
            <propertyGroup caption="Other">
                <property key="rotateEnabled" type="boolean" defaultValue="true">
                    <caption>Enable rotation</caption>
                    <description>Allow the user to pinch/rotate the map.</description>
                </property>
                <property key="scrollEnabled" type="boolean" defaultValue="true">
                    <caption>Enable scroll</caption>
                    <description>Allow the user to change the map region being displayed.</description>
                </property>
                <property key="pitchEnabled" type="boolean" defaultValue="true">
                    <caption>Enable pitch</caption>
                    <description>Allow the user to adjust the camera’s pitch angle.</description>
                </property>
            </propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Events">
            <property key="onRegionChange" type="action" required="false">
                <caption>On region change</caption>
                <description/>
            </property>
            <property key="onMarkerPress" type="action" required="false">
                <caption>On marker press</caption>
                <description/>
            </property>
        </propertyGroup>
    </properties>
</widget>
