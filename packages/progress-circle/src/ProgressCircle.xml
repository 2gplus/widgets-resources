<?xml version="1.0" encoding="utf-8"?>
<widget id="com.mendix.widget.native.progresscircle.ProgressCircle" needsEntityContext="true" offlineCapable="true" pluginWidget="true" 
    xmlns="http://www.mendix.com/widget/1.0/" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../../../xsd/widget.xsd">
    <name>Progress circle</name>
    <description>Displays progress in a circle.</description>
    <icon></icon>
    <properties>
        <property key="indeterminate" type="boolean" defaultValue="false">
            <caption>Indeterminate</caption>
            <category>Value</category>
            <description>If set, the indicator will spin and the progress value will be ignored</description>
        </property>
        <property key="progressValue" type="attribute" required="false">
            <caption>Progress value</caption>
            <category>Value</category>
            <description/>
            <attributeTypes>
                <attributeType name="Integer" />
                <attributeType name="Long" />
                <attributeType name="Decimal" />
            </attributeTypes>
        </property>
        <property key="progressMax" type="attribute" required="false">
            <caption>Maximum progress value</caption>
            <category>Value</category>
            <description>Defaults to 100</description>
            <attributeTypes>
                <attributeType name="Integer" />
                <attributeType name="Long" />
                <attributeType name="Decimal" />
            </attributeTypes>
        </property>

        <property key="animated" type="boolean" defaultValue="true">
            <caption>Animated</caption>
            <category>Appearance</category>
            <description>Whether or not to animate changes to the progress value</description>
        </property>
        <property key="size" type="integer" defaultValue="40">
            <caption>Diameter</caption>
            <category>Appearance</category>
            <description />
        </property>
        <property key="thickness" type="integer" defaultValue="3">
            <caption>Thickness</caption>
            <category>Appearance</category>
            <description />
        </property>
        <property key="showsText" type="boolean" defaultValue="false">
            <caption>Show progress text</caption>
            <category>Appearance</category>
            <description />
        </property>
        <property key="color" type="string" required="false">
            <caption>Fill color</caption>
            <category>Appearance</category>
            <description/>
        </property>
        <property key="unfilledColor" type="string" required="false">
            <caption>Unfilled color</caption>
            <category>Appearance</category>
            <description/>
        </property>
        <property key="borderWidth" type="integer" defaultValue="1">
            <caption>Border width</caption>
            <category>Appearance</category>
            <description>Set 0 to remove</description>
        </property>
        <property key="borderColor" type="string" required="false">
            <caption>Border color</caption>
            <category>Appearance</category>
            <description/>
        </property>
    </properties>
</widget>
