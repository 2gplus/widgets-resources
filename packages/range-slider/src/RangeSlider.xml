<?xml version="1.0" encoding="utf-8"?>
<widget id="com.mendix.widget.native.rangeslider.RangeSlider" supportedPlatform="Native" needsEntityContext="true" offlineCapable="true" pluginWidget="true" xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../../../xsd/widget.xsd">
    <name>Range slider</name>
    <description>Change range of values using a slider.</description>
    <icon></icon>
    <properties>
        <propertyGroup caption="General">
            <propertyGroup caption="Data source selected range">
                <property key="lowerValueAttribute" type="attribute">
                    <caption>Lower value attribute</caption>
                    <description>The selected lower bound value on the slider.</description>
                    <attributeTypes>
                        <attributeType name="Integer"/>
                        <attributeType name="Long"/>
                        <attributeType name="Decimal"/>
                    </attributeTypes>
                </property>
                <property key="upperValueAttribute" type="attribute">
                    <caption>Upper value attribute</caption>
                    <description>The selected upper bound value on the slider.</description>
                    <attributeTypes>
                        <attributeType name="Integer"/>
                        <attributeType name="Long"/>
                        <attributeType name="Decimal"/>
                    </attributeTypes>
                </property>
            </propertyGroup>
            <propertyGroup caption="Editability">
                <property key="editable" type="enumeration" defaultValue="default">
                    <caption>Editable</caption>
                    <description/>
                    <enumerationValues>
                        <enumerationValue key="default">Default</enumerationValue>
                        <enumerationValue key="never">Never</enumerationValue>
                    </enumerationValues>
                </property>
            </propertyGroup>
            <propertyGroup caption="Minimum range value">
                <property key="minimumValueAttribute" type="attribute" required="false">
                    <caption>Attribute</caption>
                    <description>The minimum value that can be selected.</description>
                    <attributeTypes>
                        <attributeType name="Integer"/>
                        <attributeType name="Long"/>
                        <attributeType name="Decimal"/>
                    </attributeTypes>
                </property>
                <property key="minimumValueDefault" type="integer" defaultValue="0">
                    <caption>Static</caption>
                    <description>Used when no value is provided through the minimum range value attribute.</description>
                </property>
            </propertyGroup>
            <propertyGroup caption="Maximum range value">
                <property key="maximumValueAttribute" type="attribute" required="false">
                    <caption>Attribute </caption>
                    <description>The maximum value that can be selected.</description>
                    <attributeTypes>
                        <attributeType name="Integer"/>
                        <attributeType name="Long"/>
                        <attributeType name="Decimal"/>
                    </attributeTypes>
                </property>
                <property key="maximumValueDefault" type="integer" defaultValue="100">
                    <caption>Static </caption>
                    <description>Used when no value is provided through the maximum range value attribute.</description>
                </property>
            </propertyGroup>
            <propertyGroup caption="Step size">
                <property key="stepSizeAttribute" type="attribute" required="false">
                    <caption>Attribute  </caption>
                    <description>The interval between two selectable points.</description>
                    <attributeTypes>
                        <attributeType name="Integer"/>
                        <attributeType name="Long"/>
                        <attributeType name="Decimal"/>
                    </attributeTypes>
                </property>
                <property key="stepSizeDefault" type="integer" defaultValue="1">
                    <caption>Static  </caption>
                    <description>Used when no value is provided through the step size attribute.</description>
                </property>
            </propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Events">
            <property key="onSlide" type="action" required="false">
                <caption>On slide</caption>
                <description>Called when the value changes while the user is sliding the handle.</description>
            </property>
            <property key="onChange" type="action" required="false">
                <caption>On change</caption>
                <description>Called when the user releases the handle and the value has changed.</description>
            </property>
        </propertyGroup>
    </properties>
</widget>
